public class f1 {
	public static void mian(String[] args) {
		int []arr1 = {1,2,3,4};
		int []arr2 = new int[3];
		System.out.print("数组arr1的值为:");
		showArray(arr1);
		System.out.print("数组arr2的值为:");
		showArray(arr2);
	}
	
	public static void showArray(int []a) {
		for(int i = 0; i < a.length; i++) {
			System.out.print(a[i]+" ");	
		}
		System.out.println();
	}
}
